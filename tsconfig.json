{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "rootDir": "src",
    },
    "include": ["src/**/*"],
    "exclude": ["**/node_modules/**"]
}